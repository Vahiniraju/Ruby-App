<div class="container">
<%= will_paginate %>


  <div class="container">
    <table class="table table-striped table-bordered">
      <thead>
      <tr>
        <th>Username</th>
        <th>Score</th>
      </tr>
      </thead>
      <tbody>
      <% @users.each  do |user| %>
        <tr>
          <td><%= user.username%></td>
          <td><%= user.score%></td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>

  <%= will_paginate %>
</div>