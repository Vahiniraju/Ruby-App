<div class="container">
  <div class="row">
    <aside class="col-md-4">

      <section class="user_info">
        <h1>
          <%= @user.first_name %>
        </h1>
        <p>Want to Delete Account?
          <% if current_user?(@user) %>
            <%= link_to "Delete account", @user, method: :delete , data: { confirm: "You sure?" } %>
          <%end%>
        </p>
      </section>
    </aside>


  </div>
</div>